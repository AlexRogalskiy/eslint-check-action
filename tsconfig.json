{
  "compilerOptions": {
      "target": "ES2017",
      "module": "commonjs",
      "noEmit": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}
